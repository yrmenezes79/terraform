resource "aws_security_group" "sg_DefaultWebserver" {
  name        = "sg_DefaultWebserver"
  description = "Trafico outbound e inbound 22"

  ingress {
    from_port   = 22
    to_port     = 22
    protocol    = "tcp"
    cidr_blocks = ["0.0.0.0/0"]
  }
   egress {
    from_port       = 0
    to_port         = 0
    protocol        = "-1"
    cidr_blocks     = ["0.0.0.0/0"]
  }
}
